<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.StudentController">
   <children>
      <Label fx:id="question_lbl" layoutX="25.0" layoutY="22.0" prefHeight="63.0" prefWidth="192.0" text="Please enter your question here:" />
      <TextField fx:id="question_txt" layoutX="25.0" layoutY="83.0" prefHeight="107.0" prefWidth="515.0" />
      <Button fx:id="send_btn" layoutX="216.0" layoutY="234.0" mnemonicParsing="false" onAction="#send" prefHeight="63.0" prefWidth="134.0" text="Send!" />
      <Label fx:id="connectionStatus_lbl" layoutX="14.0" layoutY="348.0" prefHeight="42.0" prefWidth="205.0" text="Disconnected.." />
      <Button fx:id="connect_btn" layoutX="9.0" layoutY="317.0" mnemonicParsing="false" onAction="#connectToLecturer" prefHeight="33.0" prefWidth="81.0" text="Connect" />
   </children>
</AnchorPane>
